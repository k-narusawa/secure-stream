type Query {
  userInfo: UserInfo
}

type UserInfo {
  userId: String
  profile: Profile
}

type Profile {
  familyName: String
  givenName: String
  nickname: String
  picture: String
}

type Mutation {
  changeProfile(
    familyName: String!
    givenName: String!
    nickname: String
    picture: String
  ): Profile
}